{
  "npmScope": "dominicode",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "api": {
      "tags": []
    },
    "api-data-access-auth": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-data-access-core": {
      "tags": ["scope:api", "type:data-access"]
    },
    "api-e2e": {
      "tags": []
    },
    "api-feature-auth": {
      "tags": ["scope:api", "type:feature"]
    },
    "api-feature-core": {
      "tags": ["scope:api", "type:feature"]
    },
    "web": {
      "tags": []
    },
    "web-assets": {
      "tags": ["scope:web", "type:assets"]
    },
    "web-data-access-auth": {
      "tags": ["scope:web", "type:data-access"]
    },
    "web-data-access-core": {
      "tags": ["scope:web", "type:data-access"]
    },
    "web-e2e": {
      "tags": [],
      "implicitDependencies": ["web"]
    },
    "web-feature-about": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-feature-auth": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-feature-core": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-feature-dashboard": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-feature-shell": {
      "tags": ["scope:web", "type:feature"]
    },
    "web-layout": {
      "tags": ["scope:web", "type:layout"]
    },
    "web-style": {
      "tags": ["scope:web", "type:style"]
    },
    "web-ui-form": {
      "tags": ["scope:web", "type:ui"]
    }
  }
}
